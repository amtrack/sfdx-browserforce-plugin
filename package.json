{
  "name": "sfdx-browserforce-plugin",
  "version": "0.0.0-development",
  "description": "sfdx plugin for executing various tasks using browser automation",
  "author": "Matthias Rolke <mr.amtrack@gmail.com>",
  "files": [
    "commands",
    "lib",
    "plugins"
  ],
  "main": "index.js",
  "keywords": [
    "salesforce",
    "sfdx",
    "sfdx-plugin"
  ],
  "devDependencies": {
 
    "mocha": "^5.2.0",
    "prettier": "^1.13.2"
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "mocha",
    "test:plugins": "mocha plugins/**/*.spec.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "mocha": true,
      "es6": true,
      "node": true
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "amtrack/sfdx-browserforce-plugin",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "puppeteer": "^1.4.0",
    "salesforce-alm": "^43.0.0"
  }
}
