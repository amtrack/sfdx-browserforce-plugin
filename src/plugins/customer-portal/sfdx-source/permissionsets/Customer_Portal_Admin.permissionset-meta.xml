<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Required to be set as Administrator of a Customer Portal: https://releasenotes.docs.salesforce.com/en-us/summer18/release-notes/rn_networks_manage_customer_users.htm</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Customer Portal Admin</label>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>Contact</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>ManageCssUsers</name>
    </userPermissions>
</PermissionSet>
